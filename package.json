{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "dependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "electron-is-dev": "^2.0.0",
    "knex": "^0.95.4",
    "lodash": "^4.17.21",
    "papaparse": "^5.3.1",
    "react": "^17.0.2",
    "react-confirm-alert": "^2.7.0",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-modal": "^3.13.1",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-transition-group": "^4.4.1",
    "redux": "^4.1.0",
    "redux-observable": "^2.0.0",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.6.7",
    "sqlite3": "^5.0.2",
    "styled-components": "^5.3.0",
    "styled-theming": "^2.2.0",
    "tailwindcss": "^2.1.2",
    "web-vitals": "^1.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dv": "electron .",
    "dev": "concurrently \"BROWSER=none npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "tailwind": "tailwind build src/shared/styles/tailwind.src.css -o src/shared/styles/tailwind.css",
    "models": "nodemon models.js",
    "idev": "nodemon setup/index.js",
    "db:up": "knex migrate:latest",
    "db:down": "knex migrate:rollback --all",
    "db:fresh": "npm run db:down && npm run db:up"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^6.0.2",
    "electron": "^12.0.6",
    "electron-builder": "^22.11.1",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "nodemon": "^2.0.7",
    "unimported": "^1.19.0",
    "wait-on": "^5.3.0"
  },
  "build": {
    "appId": "com.piadgs.libraryapp",
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "icon": "assets/icon.icns"
    }
  }
}
